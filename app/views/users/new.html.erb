<h1>Signup Page</h1>
<%= form_for @user, url: signup_path(@user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <br>
    <%= f.submit "Sign up" %>
<% end %>

<%= link_to "Login page", login_prompt_path %>